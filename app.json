{
  "expo": {
    "name": "setur-solution",
    "description": "Solution for Setur assessment",
    "slug": "setur-solution",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icons/icon.png",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "splash": {
        "image": "./assets/images/splash-screen/ios/splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#E02726"
      }
    },
    "android": {
      "splash": {
        "ldpi": "./assets/images/splash-screen/android/ldpi/splash.png",
        "mdpi": "./assets/images/splash-screen/android/mdpi/splash.png",
        "hdpi": "./assets/images/splash-screen/android/hdpi/splash.png",
        "xhdpi": "./assets/images/splash-screen/android/xhdpi/splash.png",
        "xxhdpi": "./assets/images/splash-screen/android/xxhdpi/splash.png",
        "xxxhdpi": "./assets/images/splash-screen/android/xxxhdpi/splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#R02726"
      }
    }
  }
}
